import string

readline : IO String
readline = __readline

print : String -> IO Unit
print = fun s : String, __print s

pure : forall A, A -> IO A
pure = fun A, fun x : A, __pure A x

bind : forall A, forall B, (A -> IO B) -> IO A -> IO B
bind = fun A, fun B, fun func : A -> IO B, fun a : IO A, __bind A B func a
